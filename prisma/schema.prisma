generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Order {
  id        Int      @id @default(autoincrement())
  orderId   Int
  symbol    String
  quantity  Int
  price     Float
  date      DateTime
  ack       Boolean
  createdAt DateTime @default(now())
}

model Stock {
  id        Int     @id @default(autoincrement())
  marketId  Int
  symbol    String
  price     Float
  createdAt DateTime @default(now())
}